<%- include('header', {here: 'collectors'}) %>

<h1>STACKSofWAX members</h1>

<div class="my-main"> 

  <table class="table">
    <tr>
      <th>Username</th>
      <th>Stack</th>
      <th>Joined</th>
    </tr>

    <% collectorsdata.forEach( (collector)=>{ %>
    <tr>
      <td>
        <a class="btn btn-outline-info" href="/collector?id=<%= collector.user_id %>">
          <img class="minipic" src="<%= collector.userimageurl %>"/><%= collector.username %>
        </a>
      </td>
      <td>?</td>
      <td><%= collector.joindate %></td>
    </tr>
    <% }); %>
  </table>

</div>

<%- include('footer') %>